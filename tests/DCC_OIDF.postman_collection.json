			"name": "entity_statement_contents",
			"item": [
				{
					"name": "Validity and signature of Entity Configuration",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"",
									"let jwt = 'eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJzdWIiOiIxMjM0NTY3ODkwIiwibmFtZSI6IkpvaG4gRG9lIiwiaWF0IjoxNTE2MjM5MDIyfQ.SflKxwRJSMeKKF2QT4fwpMeJf36POk6yJV_adQssw5c'",
									"jwt = 'eyJraWQiOiJzYW5kYm94LXRhLTEiLCJ0eXAiOiJlbnRpdHktc3RhdGVtZW50K2p3dCIsImFsZyI6IkVTMjU2In0..KigahJo9v2tg0ewlBK-z2exYSNuAiZsIlW_-RkqFqfje31kCZMLctqNPQGwCk1s4z8d4njTGi70RNPfFTzgSeg'",
									"",
									"function parseJwt (token, part) {",
									"    var base64Url = token.split('.')[part];",
									"    var base64 = base64Url.replace(/-/g, '+').replace(/_/g, '/');",
									"    var jsonPayload = decodeURIComponent(atob(base64).split('').map(function(c) {",
									"        return '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2);",
									"    }).join(''));",
									"",
									"    return JSON.parse(jsonPayload);",
									"}",
									"",
									"console.log(parseJwt(jwt,0))",
									"console.log(parseJwt(jwt,1))",
									"console.log(parseJwt(jwt,1)['jwks']['keys'][0])"
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"require('atob')"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{TEST_URL}}/{{TRUST_ANCHOR_NAME}}/.well-known/openid-federation",
